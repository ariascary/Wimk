<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="cap.aplication.wimk.Registro">


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/app_description"
        android:scaleType="centerCrop"
        android:src="@drawable/fondo" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/wink"
            android:layout_width="110dp"
            android:layout_height="55dp"
            android:layout_marginBottom="16dp"
            android:fontFamily="@font/impact"
            android:text="@string/app_name"
            android:textColor="@color/darkblue"
            android:textSize="40sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.052"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.021" />

        <Button
            android:id="@+id/registro26"
            android:layout_width="153dp"
            android:layout_height="53dp"
            android:fontFamily="@font/letra"
            android:includeFontPadding="false"
            android:text="@string/guardar"
            android:textColor="@color/lightblue"
            android:textSize="16sp"
            android:textStyle="bold"
            android:typeface="normal"
            app:backgroundTint="@color/darkblue"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.941"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.986"
            app:strokeColor="@color/lightblue"
            tools:fontFamily="@font/letra"
            tools:ignore="DuplicateSpeakableTextCheck" />

        <ImageButton
            android:id="@+id/imageButton3351"
            android:layout_width="68dp"
            android:layout_height="50dp"
            android:layout_marginBottom="12dp"
            android:padding="0.1dp"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.046"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/flecha"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/textView36"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="100dp"
            android:fontFamily="@font/letra"
            android:text="@string/Etiquetas"
            android:textColor="@color/darkblue"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <CheckBox
            android:id="@+id/checkBox766"
            android:layout_width="380dp"
            android:layout_height="55dp"
            android:fontFamily="@font/letra"
            android:text="Receta vegetariana"
            android:textAlignment="textStart"
            android:textColor="@color/darkblue"
            android:textSize="24sp"
            android:textStyle="bold"
            app:buttonTint="@color/darkblue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.483"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="159dp" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout2"
            android:layout_width="320dp"
            android:layout_height="48dp"
            android:layout_marginTop="504dp"
            android:background="#2DCDDAEF"
            app:boxBackgroundColor="#1F4E79"
            app:boxBackgroundMode="outline"
            app:boxStrokeColor="@color/black"
            app:helperText="Duración máxima de la receta"
            app:helperTextEnabled="true"
            app:helperTextTextColor="@color/darkblue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.175"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout1"
            android:layout_width="320dp"
            android:layout_height="48dp"
            android:layout_marginTop="564dp"
            android:background="#32CDDAEF"
            app:boxBackgroundColor="#1F4E79"
            app:boxBackgroundMode="outline"
            app:boxStrokeColor="@color/black"
            app:helperText="Kcal máximas"
            app:helperTextEnabled="true"
            app:helperTextTextColor="@color/darkblue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.175"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <CheckBox
            android:id="@+id/checkBox628"
            android:layout_width="380dp"
            android:layout_height="55dp"
            android:fontFamily="@font/letra"
            android:text="Receta vegana"
            android:textAlignment="textStart"
            android:textColor="@color/darkblue"
            android:textSize="24sp"
            android:textStyle="bold"
            app:buttonTint="@color/darkblue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.483"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="227dp" />

        <CheckBox
            android:id="@+id/checkBox1659"
            android:layout_width="380dp"
            android:layout_height="55dp"
            android:fontFamily="@font/letra"
            android:text="Ni vegetariana ni vegana"
            android:textAlignment="textStart"
            android:textColor="@color/darkblue"
            android:textSize="24sp"
            android:textStyle="bold"
            app:buttonTint="@color/darkblue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.516"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="295dp" />

        <CheckBox
            android:id="@+id/checkBox6559"
            android:layout_width="380dp"
            android:layout_height="55dp"
            android:fontFamily="@font/letra"
            android:text="Es un plato principal"
            android:textAlignment="textStart"
            android:textColor="@color/darkblue"
            android:textSize="24sp"
            android:textStyle="bold"
            app:buttonTint="@color/darkblue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.483"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="435dp" />

        <CheckBox
            android:id="@+id/checkBox66669"
            android:layout_width="380dp"
            android:layout_height="55dp"
            android:fontFamily="@font/letra"
            android:text="Es un postre"
            android:textAlignment="textStart"
            android:textColor="@color/darkblue"
            android:textSize="24sp"
            android:textStyle="bold"
            app:buttonTint="@color/darkblue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.483"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="364dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>