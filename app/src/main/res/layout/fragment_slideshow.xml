<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="cap.aplication.wimk.Registro">


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/app_description"
        android:scaleType="centerCrop"
        android:src="@drawable/fondo" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/wink"
            android:layout_width="110dp"
            android:layout_height="55dp"
            android:layout_marginBottom="16dp"
            android:fontFamily="@font/impact"
            android:text="@string/app_name"
            android:textColor="@color/darkblue"
            android:textSize="40sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.052"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.021" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/jaj1111"
            android:layout_width="320dp"
            android:layout_height="48dp"
            android:layout_marginTop="160dp"
            android:background="#8BCDDAEF"
            app:boxBackgroundColor="#1F4E79"
            app:boxBackgroundMode="outline"
            app:boxStrokeColor="@color/black"
            app:helperText="@string/nombre"
            app:helperTextEnabled="true"
            app:helperTextTextColor="@color/darkblue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout888"
        android:layout_width="320dp"
        android:layout_height="48dp"
        android:layout_marginTop="228dp"
        android:background="#8BCDDAEF"
        app:boxBackgroundColor="#1F4E79"
        app:boxBackgroundMode="outline"
        app:boxStrokeColor="@color/black"
        app:helperText="@string/apellido"
        app:helperTextEnabled="true"
        app:helperTextTextColor="@color/darkblue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout98"
            android:layout_width="320dp"
            android:layout_height="48dp"
            android:layout_marginTop="228dp"
            android:background="#8BCDDAEF"
            app:boxBackgroundColor="#1F4E79"
            app:boxBackgroundMode="outline"
            app:boxStrokeColor="@color/black"
            app:helperText="@string/apellido"
            app:helperTextEnabled="true"
            app:helperTextTextColor="@color/darkblue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.505"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout87"
            android:layout_width="320dp"
            android:layout_height="48dp"
            android:layout_marginTop="376dp"
            android:background="#8BCDDAEF"
            app:boxBackgroundColor="#1F4E79"
            app:boxBackgroundMode="outline"
            app:boxStrokeColor="@color/black"
            app:helperText="@string/nombreusuario"
            app:helperTextEnabled="true"
            app:helperTextTextColor="@color/darkblue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.505"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout78"
            android:layout_width="320dp"
            android:layout_height="48dp"
            android:layout_marginTop="300dp"
            android:background="#8BCDDAEF"
            app:boxBackgroundColor="#1F4E79"
            app:boxBackgroundMode="outline"
            app:boxStrokeColor="@color/black"
            app:helperText="@string/email"
            app:helperTextEnabled="true"
            app:helperTextTextColor="@color/darkblue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.516"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button3313"
            android:layout_width="319dp"
            android:layout_height="57dp"
            android:fontFamily="@font/letra"
            android:text="@string/CambiarPrefe"
            android:textAlignment="textStart"
            android:textColor="@color/darkblue"
            android:textSize="16sp"
            app:backgroundTint="#95CDDAEF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.489"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="450dp" />

        <Button
            android:id="@+id/button1662"
            android:layout_width="319dp"
            android:layout_height="57dp"
            android:fontFamily="@font/letra"
            android:text="@string/ModificarRece"
            android:textAlignment="textStart"
            android:textColor="@color/darkblue"
            android:textSize="16sp"
            app:backgroundTint="#9ACDDAEF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.51"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="521dp" />

        <Button
            android:id="@+id/registro27"
            android:layout_width="153dp"
            android:layout_height="53dp"
            android:fontFamily="@font/letra"
            android:includeFontPadding="false"
            android:text="@string/guardar"
            android:textColor="@color/lightblue"
            android:textSize="16sp"
            android:textStyle="bold"
            android:typeface="normal"
            app:backgroundTint="@color/darkblue"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.937"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.986"
            app:strokeColor="@color/lightblue"
            tools:fontFamily="@font/letra"
            tools:ignore="DuplicateSpeakableTextCheck" />

        <ImageButton
            android:id="@+id/imageButton651"
            android:layout_width="68dp"
            android:layout_height="50dp"
            android:layout_marginBottom="12dp"
            android:padding="0.1dp"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.046"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/flecha"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="104dp"
            android:fontFamily="@font/letra"
            android:text="@string/Perfil"
            android:textColor="@color/darkblue"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>