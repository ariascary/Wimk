                    android:layout_height="wrap_content"
                    android:fontFamily="@font/letra"
                    android:text="@string/Almendras"
                    android:textColor="@color/darkblue"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    app:buttonTint="@color/darkblue" />

                <CheckBox
