<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cap.aplication.wimk.Buscar_ingredientes" >

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/app_description"
        android:scaleType="centerCrop"
        android:src="@drawable/fondo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/imageButton100"
        android:layout_width="68dp"
        android:layout_height="50dp"
        android:layout_marginBottom="12dp"
        android:padding="0.1dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/flecha"
        tools:ignore="SpeakableTextPresentCheck" />


    <TextView
        android:id="@+id/text_home"
        android:layout_width="350dp"
        android:layout_height="104dp"
        android:fontFamily="@font/letra"
        android:text="@string/EligeIngre"
        android:textAlignment="center"
        android:textColor="@color/darkblue"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.094" />

    <CheckBox
        android:id="@+id/checkBox71"
        android:layout_width="311dp"
        android:layout_height="40dp"
        android:layout_marginTop="476dp"
        android:fontFamily="@font/letra"
        android:inputType="textMultiLine"
        android:lines="15"
        android:shadowColor="@color/lightblue"
        android:text="@string/VEgetar"
        android:textAlignment="viewStart"
        android:textColor="@color/darkblue"
        android:textColorHint="@color/darkblue"
        app:buttonIconTint="@color/lightblue"
        app:buttonTint="@color/darkblue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView733"
        android:layout_width="311dp"
        android:layout_height="40dp"
        android:layout_marginTop="352dp"
        android:layout_marginBottom="24dp"
        android:fontFamily="@font/letra"
        android:hint="@string/PonerFiltros"
        android:textColor="@color/grey"
        android:textColorHint="@color/grey"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/nombrePerfil"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/checkBox70"
        android:layout_width="311dp"
        android:layout_height="40dp"
        android:layout_marginTop="428dp"
        android:fontFamily="@font/letra"
        android:inputType="textMultiLine"
        android:lines="15"
        android:shadowColor="@color/lightblue"
        android:text="@string/VEgana"
        android:textAlignment="viewStart"
        android:textColor="@color/darkblue"
        android:textColorHint="@color/darkblue"
        app:buttonIconTint="@color/lightblue"
        app:buttonTint="@color/darkblue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/checkBox67"
        android:layout_width="311dp"
        android:layout_height="40dp"
        android:layout_marginTop="376dp"
        android:fontFamily="@font/letra"
        android:inputType="textMultiLine"
        android:lines="15"
        android:shadowColor="@color/lightblue"
        android:text="@string/SoloPostre"
        android:textAlignment="viewStart"
        android:textColor="@color/darkblue"
        android:textColorHint="@color/darkblue"
        app:buttonIconTint="@color/lightblue"
        app:buttonTint="@color/darkblue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/multiValueEditText"
        android:layout_width="323dp"
        android:layout_height="47dp"
        android:layout_marginTop="236dp"
        android:background="#90CDDAEF"
        android:fontFamily="@font/letra"
        android:gravity="top"
        android:hint="@string/Introduceingre"
        android:inputType="textMultiLine"
        android:maxLines="15"
        android:minLines="5"
        android:textAlignment="viewStart"
        android:textColor="@color/darkblue"
        android:textColorHint="@color/darkblue"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.181"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView799"
        android:layout_width="339dp"
        android:layout_height="39dp"
        android:layout_marginTop="192dp"
        android:layout_marginBottom="24dp"
        android:fontFamily="@font/letra"
        android:hint="@string/instruccionesIngredientes"
        android:textColor="@color/grey"
        android:textColorHint="@color/grey"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/nombrePerfil"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.222"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button19"
        android:layout_width="126dp"
        android:layout_height="52dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@color/darkblue"
        android:fontFamily="@font/letra"
        android:text="@string/Buscar"
        android:textColor="@color/lightblue"
        android:textSize="16sp"
        android:textStyle="bold"
        app:backgroundTint="@color/darkblue"
        app:iconTint="@color/darkblue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>
